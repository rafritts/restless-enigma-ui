		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@fortawesome/svelte-fontawesome": "^0.2.1",
		"@stomp/stompjs": "^7.0.0",
		"autoprefixer": "^10.4.17",
		"daisyui": "^4.6.0",
		"postcss": "^8.4.33",
		"sockjs-client": "^1.6.1",
		"tailwindcss": "^3.4.1"
	}
}
